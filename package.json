{
  "name": "@pulumi/cdk",
  "version": "0.0.1",
  "description": "Pulumi/CDK Interop SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pulumi/pulumi-cdk.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pulumi/pulumi-cdk/issues"
  },
  "homepage": "https://github.com/pulumi/pulumi-cdk#readme",
  "engines": {
    "node": ">=8.13.0 || >=10.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "build": "tsc && cp package.json lib/",
    "lint": "./node_modules/.bin/eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "test-examples": "cd lib && yarn link && cd .. && cd examples && go test -timeout 30m -v ."
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@pulumi/aws-native": "^0.15.0",
    "@pulumi/pulumi": "^3.28.0",
    "aws-cdk-lib": "^2.20.0",
    "constructs": "^10.0.111"
  }
}
